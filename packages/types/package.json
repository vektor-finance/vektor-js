{
  "name": "@vektor-finance/types",
  "version": "0.17.0",
  "description": "Common types for all Vektor JavaScript SDKs",
  "repository": "git://github.com/vektor-finance/vektor-js.git",
  "homepage": "https://github.com/vektor-finance/vektor-js/tree/master/packages/types",
  "author": "Vektor, Inc.",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=6"
  },
  "main": "dist/index.js",
  "module": "esm/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "build": "run-p build:es5 build:esm",
    "build:es5": "tsc -p tsconfig.build.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:watch": "run-p build:watch:es5 build:watch:esm",
    "build:watch:es5": "tsc -p tsconfig.build.json -w --preserveWatchOutput",
    "build:watch:esm": "tsc -p tsconfig.esm.json -w --preserveWatchOutput",
    "link:yarn": "yarn link",
    "lint": "run-s lint:prettier lint:eslint",
    "lint:prettier": "prettier --check \"{src,test}/**/*.ts\"",
    "lint:eslint": "eslint . --cache --cache-location '../../eslintcache/' --format stylish",
    "fix": "run-s fix:eslint fix:prettier",
    "fix:prettier": "prettier --write \"{src,test}/**/*.ts\"",
    "fix:eslint": "eslint . --format stylish --fix",
    "pack": "npm pack",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@vektor-finance/api": "^0.17.0"
  },
  "devDependencies": {
    "@vektor-finance/eslint-config-sdk": "^0.17.0",
    "lint-staged": "12.4.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.6.2",
    "typescript": "4.6.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --quiet"
    ],
    "*.{js,jsx,ts,tsx,json}": [
      "prettier --write"
    ]
  },
  "sideEffects": false
}
